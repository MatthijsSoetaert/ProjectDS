<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="126.0" prefWidth="364.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.PasswordController">
   <children>
      <VBox layoutX="57.0" layoutY="32.0" prefHeight="62.0" prefWidth="251.0">
         <children>
            <Label text="Password:" />
            <TextField fx:id="passwordTextField">
               <VBox.margin>
                  <Insets bottom="7.0" />
               </VBox.margin></TextField>
            <ButtonBar prefHeight="40.0" prefWidth="364.0">
              <buttons>
                <Button mnemonicParsing="false" onAction="#confirmPassword" text="Confirm" />
              </buttons>
            </ButtonBar>
            <Label fx:id="ErrorMessageTextField" textFill="RED" />
         </children>
      </VBox>
   </children>
</AnchorPane>
